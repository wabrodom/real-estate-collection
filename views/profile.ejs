<%- include('partials/header') -%>
<div class="container">
  <div class="row mt-5">
      <div class="col-10">

        <div class="mb-5">
            <p><strong>User Name</strong>: <%= user.userName %></p>
            <p><strong>Email</strong>: <%= user.email %></p>
            <a href="/logout" class="col-3 btn btn-primary">Logout</a>
        </div>
        
      
          <h1>Add an entrepreneur</h1>
        

            <form action="/post/createPost" enctype="application/x-www-form-urlencoded" method="POST">
              <%- include('partials/formContent', {post: undefined, poster: true}) -%>
              <button type="submit" class="btn btn-accent" value="Upload">Submit</button>
            </form>
      
      </div>

    <div class="table-responsive mt-3">
      <%- include('partials/table', {posts: posts}) -%> 
    </div> 

      
        <div class="row justify-content-center mt-5">
          <a class="btn btn-primary" href="/feed">Return to Feed</a>
        </div>  

    </div>
  </div>
</div>
<%- include('partials/footer') -%>